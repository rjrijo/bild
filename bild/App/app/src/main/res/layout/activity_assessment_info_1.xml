                    android:layout_height="wrap_content"
                    android:text="@string/rd_btn_no"
                    android:id="@+id/radioButtonLeadership_series_Oral_review_no" />
            </LinearLayout>

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/bg_login2" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="@string/assessment_done"
                android:id="@+id/textViewLeadership_series_Assessment_done"

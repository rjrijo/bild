<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="${relativePackage}.${activityClass}" >

    <Button
        android:id="@+id/btnEnter_trainees_details"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/btn_logut_bg"
        android:text="@string/btn_enter_trainees_details"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="15dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView3"
        android:layout_below="@+id/btnEnter_trainees_details"
        android:layout_above="@+id/btnLogout" >

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_below="@+id/btnEnter_trainees_details"
            android:layout_above="@+id/btnLogout">

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <Button
                        android:id="@+id/btnGeneral_details"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/btn_logut_bg"
                        android:text="@string/btn_general_details"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="15dp"
                        android:onClick="EnterGeneralDetails"
                        android:layout_below="@+id/btnEnter_edit_trainees_details"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="2.5dp" />

                    <Button
                        android:id="@+id/btnTraining_details"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/btn_logut_bg"
                        android:text="@string/btn_training_details"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="15dp"
                        android:onClick="EnterTrainingDetails"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="2.5dp"
                        android:layout_marginRight="5dp" />
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <Button
                        android:id="@+id/btnAssessment_info_1"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/btn_logut_bg"
                        android:text="@string/btn_assessment_info_1"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="15dp"
                        android:onClick="EnterAssessmentInfo1"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="2.5dp" />

                    <Button
                        android:id="@+id/btnAssessment_info_2"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/btn_logut_bg"
                        android:text="@string/btn_assessment_info_2"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="15dp"
                        android:onClick="EnterAssessmentInfo2"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="2.5dp"
                        android:layout_marginRight="5dp" />
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <Button
                        android:id="@+id/btnOfficial_details"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/btn_logut_bg"
                        android:text="@string/btn_official_details"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="15dp"
                        android:onClick="EnterOfficialDetails"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="2.5dp" />

                    <Button
                        android:id="@+id/btnAdditional_information"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/btn_logut_bg"
                        android:text="@string/btn_additional_information"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="15dp"
                        android:onClick="EnterAdditionalInformation"
                        android:layout_below="@+id/btnEnter_edit_trainees_details"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="2.5dp"
                        android:layout_marginRight="5dp" />
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <Button
                        android:id="@+id/btnTestimony"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/btn_logut_bg"
                        android:text="@string/btn_testimony"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="15dp"
                        android:onClick="EnterTestimony"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginTop="5dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="2.5dp"
                        android:layout_weight="1"/>

                    <Button
                        android:id="@+id/btnFamily_details"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/btn_logut_bg"
                        android:text="@string/btn_family_details"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="15dp"
                        android:onClick="EnterFamilyDetails"
                        android:layout_below="@+id/btnEnter_edit_trainees_details"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:layout_marginLeft="2.5dp"
                        android:layout_marginRight="5dp"
                        android:layout_weight="1"/>
                </TableRow>
            </TableLayout>

        </LinearLayout>
    </ScrollView>

    <Button
        android:id="@+id/btnLogout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/btn_logut_bg"
        android:text="@string/btn_logout"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="15dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="back" />


</RelativeLayout>